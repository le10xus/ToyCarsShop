{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictFunctionTypes": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"

}